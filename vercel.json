{
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/auth/:path*",
      "destination": "/auth/:path*"
    },
    {
      "source": "/admin/:path*",
      "destination": "/AdminPage/:path*"
    },
    {
      "source": "/AdminPage/:path*",
      "destination": "/AdminPage/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/htmlPage/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}