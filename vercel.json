{
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/auth/:path*",
      "destination": "/auth/:path*"
    },
    {
      "source": "/AdminPage/:path*",
      "destination": "/AdminPage/:path*"
    },
    {
      "source": "/htmlPage/:path*",
      "destination": "/htmlPage/:path*"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/htmlPage/$1",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      }
    }
  ],
  "trailingSlash": false
}